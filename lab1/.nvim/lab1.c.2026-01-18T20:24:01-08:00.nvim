#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
  char *myLine = NULL;
  char *storage = NULL;
  size_t length = 0;

  printf("Please enter some text: ");
  getline(&myLine, &length, stdin);
  printf("Tokens:\n");

  char *current = strtok_r(myLine, " ", &storage);
  while (current != NULL) {
    printf("%s\n", storage);
    current = strtok_r(NULL, " ", &storage);
  }
}
