#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
  char myLine[100];
  char *storage = NULL;

  printf("Please enter some text: ");
  getline(&myLine, 0, stdin);
  printf("Tokens:\n");

  char *current = strtok_r(myLine, " ", &storage);
  while (current != NULL) {
    printf("%s\n", storage);
    current = strtok_r(NULL, " ", &storage);
  }
}
