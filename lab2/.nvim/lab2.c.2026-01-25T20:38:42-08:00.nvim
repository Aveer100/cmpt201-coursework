#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
  char* line = NULL;
  char* temp = NULL;
  size_t lenght = 0;
  
  while(100)
  {
    printf("Enter programs to run.\n");
    getline(&line, &lenght, stdin);
    char* entry = strtok_r(line, "\n", &temp);
    pid = fork();
    if(pid == 0)
    {
      execl(entry, entry, (char*)NULL);
      printf("Exec failure\n");
      exit(1);
    }
    else
    { 
      waitpid(pid, NULL, 0);
    }
  }
  free(line)
  return 0;
}



